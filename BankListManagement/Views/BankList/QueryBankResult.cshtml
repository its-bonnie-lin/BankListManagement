@using BankListManagement.Models
@model BankListManagement.Models.QueryBankResult

@{
    ViewBag.Title = "QueryBankResult";
}

<h2>QueryBankResult</h2>



<div class="scb-table-body stb-thbg">
    <div class="scb-form sf-2column">
        <dl>
            <dt class="oneline-inp">銀行代號</dt>
            <dd>
                <label class="def-select">
                    @Html.TextBoxFor(t => t.BankCode)
                </label>
            </dd>
        </dl>
        <dl>
            <dt class="oneline-inp">金融機構</dt>
            <dd>
                <label class="def-select">
                    @Html.TextBoxFor(t => t.Bank, new { })
                </label>
            </dd>
        </dl>
    </div>
</div>
<div>
    <a href="/BankList/BankListIndex">按我返回</a>
</div>


